        },
        "motorTorqueControl": {
            "useForYawControl": true
            "yawControl": {
                "yawControlledMotors": ["mainEngine", "frontMotor", "rearMotor"]
                "PIDSettings": [
                    ["type",        "kP",   "kI",   "kD",   "integralInCoef",   "integralOutCoef"],
                    ["yawAV",       1.0,    0.7,     0.0,   50,                20,              ]
                    ["slipAngle",   2.0,    1.0,     0.0,   10,                 1,               ]
                ],
                "yawAVThreshold": 0.4
                "slipAngleThreshold": 0.05
            }
